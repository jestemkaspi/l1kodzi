{% extends '@layout/default.html.twig' %}

{% block title %}Home{% endblock %}

{% block content %}
    <div class="jumbotron">
        <h1>Welcome to login page</h1>
    </div>

    <div class="row">
        <div class="col-md-1">
<form>
    <input type="text" />
    <input type="password" />
</form>
        </div>
    </div>

    <ul>
        {% if users is empty %}
            <li>no users in database</li>
        {% else %}
            {% for user in users %}
                <li>{{ user.login }}</li>
            {% endfor %}
        {% endif %}
    </ul>
{% endblock %}
